@charset "euc-kr";
@import url("reset.css");
@import url("https://fonts.googleapis.com/css?family=Open+Sans:400,700");
@import url("http://fonts.googleapis.com/earlyaccess/notosanskr.css");
*{box-sizing:border-box}

/* Noto Sans KR (Korean) */
nav{position: fixed;top:0;height: 60px;background: #fff;border-bottom: 1px solid #e9e9e9;width: 100%;}
nav ul{width: 300px;margin: 0 auto}
nav li{float: left;width: 100px;}
nav li a{color: #23232e;font-size: 18px;line-height: 60px;}
nav li a:hover{color: #7b77c9;}
.yunsung_wrap{overflow: hidden;}
.yunsung {width: 940px;margin: 120px auto 0;overflow: hidden;}
.yunsung h1{text-align: left;margin-top: 26px;font-family:'Open Sans';font-size:18px;color: #23232e;}
.yunsung .txt{margin-top: 15px;text-align: left;font-family:'Noto Sans KR';color: #888a8b;font-size: 14px;line-height: 20px;}
.yunsung .txt strong{font-weight: 700;}
.yunsung_img{background: #ededed;padding: 60px 0;margin-top: 57px;}
.yunsung_img img{width: 940px;margin: 0 auto;}
.yunsung_wrap .link{font-family:'Noto Sans KR';font-size:18px;background: #23232e;color: #fff;padding: 2px 5px 4px 5px;}
.yunsung_wrap .link:hover{background: #7b77c9;}

footer{height: 50px}
footer .copyright{font-family:'Open Sans';font-size:14px;text-align: center;line-height: 50px;color: #7c7c7c;}