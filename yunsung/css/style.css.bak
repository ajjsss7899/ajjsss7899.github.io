@charset "euc-kr";
@import url("reset.css");
@import url("https://fonts.googleapis.com/earlyaccess/nanumgothic.css");
*{box-sizing:border-box}
body{overflow-x:hidden}
body,a,input,label{font-family:'Nanum Gothic';letter-spacing:-1px}
.blind{overflow:hidden;font-size:1px;line-height:1px;text-indent:-1000000px}
.scroll{overflow-y:hidden;height:100%}
/* util_menu */
@media all and (max-width: 1220px){
header{margin:0 10px}
}
.header_section{width:1200px;max-width:100%;margin:0 auto}
.util_menu{position:relative;z-index:1000;z-index:1001;font-size:0;text-align:right}
.util_menu li{display:inline-block}
.util_menu a{display:block;padding:0 6px;background:url(../images/btn_other_line.png) left center no-repeat;font-size:12px;line-height:40px;color:#c6c6c6}
.util_menu li:first-child a{padding:0 6px 0 0;background:none}
.util_menu li:last-child a{padding:0 0 0 6px}

/* 글로벌 네비게이션 */
nav{overflow:hidden;float:left;position:absolute;z-index:101;z-index:1003;width:1200px;max-width:100%;}
nav.on{position:fixed;top:0}
nav h1{position:absolute;left:0}
.gnb{overflow:hidden;position:relative;height:100%;margin:0 60px;background:#fff;text-align:center;border-bottom:1px solid #e1e1e1;}
.gnb:hover{border-bottom: 1px solid #c6c6c6;}
.gnb a{display:block;font-size:15px;line-height:59px;color:#666}
.gnb > li{display:block;float:left;width:12.5%}
.gnb > li > a{background:url(../images/gnb_line.gif) left center no-repeat}
.gnb > li:first-child > a{background:none}
.gnb > li:last-child > a{border-right:1px solid #e1e1e1;box-sizing:border-box}
.gnb > li > ul{display:none;height:288px;border-top:1px solid #c6c6c6;box-sizing:border-box}
.gnb > li > ul.on{display:block!important;border-bottom:0;}
.gnb > li:first-child > ul{border-left:1px solid #c6c6c6}
.gnb > .bdr1{border-right:1px solid #c6c6c6}
.gnb > .bdr1 a{display: block;}
.gnb > li > ul > li{text-align:left}
.gnb > li > ul > li > a{margin:0 10px;border-top:1px solid #e2e2e2;background:#fff;font-size:13px;line-height:40px}
.gnb > li > ul > li > a:hover{font-weight:bold;color:#666}
.gnb > li > ul > li:first-child a{border:0}
.srch{position:absolute;top:0;right:0;border-bottom: 1px solid #e1e1e1;border-right: 1px solid #e1e1e1;overflow: hidden;}
.srch a{display:inline-block;background:#fff;margin-bottom: -4px;margin-right: -1px;}
.srch.on{display:block}
.sitemap{display:none}
.gnb .close{display:none}
.request{display:none}

@media all and (max-width: 1220px){
.gnb{margin:0 80px 0 60px}
.srch{right:10px;margin-right:10px}
}

/* 1023px util_menu */
@media all and (max-width: 1023px){
header{width:100%;margin:0}
.util_menu{display:none;position:fixed;top:0;left:0;z-index:1005;width:270px;height:51px;margin-left:-270px;background:#454345}
.util_menu a{background:none}
.util_menu.on{display:block}
.util_menu li{float:left;text-align:center}
.util_menu li:last-child{display:none}
.util_menu a{width:135px;height:51px;border-bottom:1px solid #000;border-left:1px solid #000;font-size:13px;line-height:50px;color:#fff}
.util_menu li:first-child a{border-left:0}
}

/* 1023px 네비게이션 */
@media all and (max-width: 1023px){
.m_nav{left:0;z-index:1003;height:100%;margin-left:-270px}
.m_nav.on{position:fixed}
nav{overflow:visible;clear:both;/* position:static; */width:auto}
nav.on{position:static}
nav h1{position:fixed;z-index:1002;width:100%}
nav h1 a{display:block;width:100%;height:51px;background:#454345}
nav h1 img{width:50px;margin:0 auto}
.gnb{display:block;display:none;overflow:visible;overflow:auto;width:270px;height:100%;margin:0;padding-bottom:100px;background:#302f30}
.gnb.on{display:block;margin-top: 51px;}
.gnb a{display:block;width:100%;padding:14px 0 14px 32px;border-bottom:1px solid #03070c;line-height:1;font-size:15px;font-weight:bold;color:#919191;text-align:left}
.gnb > li{display:block;clear:both;position:relative;width:100%}
.gnb > li > a{background:none}
.gnb > li:last-child > a{border:0}
.gnb > li > ul{height:auto;border:0}
.gnb > li:first-child > ul{border:0}
.gnb > li > ul > li > a{margin:0;border:0;background:#1b2126;line-height:1;font-size:13px;color:#fff}
.gnb > li > ul > li > a:hover{color:#fff}
.srch{position:static}
.gnb > .bdr1{border:0}
.gnb > li > ul{display:none}
.nav_ico{position:absolute;top:15px;right:14px;width:13px;height:13px;background:url(../images/ico_sitemap_open2.png) center center no-repeat}
.gnb > li.on > ul{display:block}
.gnb > li.on > a{border-bottom:2px solid #000;background:#222;color:#fff}
.gnb > li.on .nav_ico{background:url(../images/ico_sitemap_close2.png) center center no-repeat}
.sitemap{display:block;position:fixed;top:7px;left:3px;z-index:1004;width:44px;height:36px;border:0;background:url(../images/m_sitemap.png) center center no-repeat;background-size:50%;cursor:pointer}
.srch{display:none}
.gnb .close{display:block}
.request{display:block;z-index:1002}
}

/* 1023px 상담신청 */
@media all and (max-width: 1023px){
.request{position:fixed;width:100%;margin-top:51px}
.request li{float:left;width:25%;background-size:80px auto;text-align:center}
.request li a{display:block;height:65px;padding-top:42px;font-size:12px;line-height:12px;color:#fff}
.request1{background:#82aa15 url(../images/btn_m_request_icon01.png) center center no-repeat}
.request2{background:#d43e4e url(../images/btn_m_request_icon02.png) center center no-repeat}
.request3{background:#e49223 url(../images/btn_m_request_icon03.png) center center no-repeat}
.request4{background:#329cba url(../images/btn_m_request_icon04.png) center center no-repeat}
}

/* main_vis */
.main_vis_wrap{overflow:hidden}
.main_vis{left:50%;width:1903px;height:770px;margin-left:-951.5px;margin-top:-41px}
.main_vis .slick-dots{display:inline;position:relative;left:892px;bottom:638px;z-index:1000}
.main_vis .slick-dots li{width:14px;height:14px}
.main_vis .slick-dots li button{width:14px;height:14px;background:url(../images/main_vis_dots1.png) no-repeat;background-position:center top}
.main_vis .slick-dots li.slick-active button{background-position:center bottom}
.main_vis .slick-prev,.main_vis .slick-next{display:none!important}

/* 1023px main_vis */
@media all and (max-width: 1023px){
.main_vis{clear:both;height:550px;margin-left:-679.5px;margin-top:0}
.main_vis img{height:550px!important}
}

/* 639px main_vis */
@media all and (max-width: 639px){
.main_vis{height:385px;margin-left:-476px}
.main_vis img{height:385px!important}
}

/* case 사례 */
.case{overflow:hidden;margin-top:80px}
.case_lst{overflow:hidden;width:397px;margin:0 auto}
.case_lst li{float:left;width:25%;text-align:center}
.case_lst li.on a{color:#333}
.case_lst li.on span{background-position:center bottom}
.case_lst a{font-size:13px;line-height:13px;color:#888}
.case_lst span{display:block;width:50px;height:50px;margin:0 auto;margin-bottom:10px;background-position:center top;background-size:50px auto}
.case_lst1{background:url(../images/main_ex_tab01.png) no-repeat}
.case_lst2{background:url(../images/main_ex_tab02.png) no-repeat}
.case_lst3{background:url(../images/main_ex_tab03.png) no-repeat}
.case_lst4{background:url(../images/main_ex_tab04.png) no-repeat}
.case_slider{width:1210px;max-width:100%;margin:0 auto;margin-top:29px;padding:0 38px}
.case_slider .slick-prev{left:10px;width:23px;height:41px;background:url(../images/btn_slide_prev01.png) 0 0 no-repeat}
.case_slider .slick-next{right:10px;width:23px;height:41px;background:url(../images/btn_slide_next01.png) 0 0 no-repeat}
.case_slider_area{margin:0 5px}
.case_slider_area img{width:100%}
.case_slider_area a{display:block}
.case_txt{overflow:hidden;height:68px;border:1px solid #e1dcd6;border-top:none;text-align:center}
.case_txt .txt1{margin-top:16px;font-size:14px;color:#5e5c57}
.case_txt .txt2{margin-top:5px;font-size:13px;color:#5e5c57}

/* 639px case 사례 */
@media all and (max-width: 639px){
.case{margin-top:40px}
.case_lst{width:auto}
.case_lst li{width:25%}
.case_lst span{width:25px;height:25px;margin-bottom:10px;background-size:25px auto}
}

/* contents1 */
.contents1_wrap{margin-top:49px;margin-left:3px}
.contents1{overflow:hidden;width:1203px;max-width:100%;margin:0 auto}
.contents1 article{height: 461px;max-height: 100%;}
.contents1 h2{margin-top:22px;font-size:20px;font-weight:bold;color:#444;text-align:center}
.contents1_area1{float:left;width:25%;border:1px solid #ddd}
.contents1_lst{margin-top:17px}
.contents1_lst ul{font-size:0;text-align:center}
.contents1_lst li{display:inline-block}
.contents1_lst li.on a{font-weight:bold;color:#666}
.contents1_lst a{padding:0 7px;background:url(../images/list_tab_line.gif) center left no-repeat;font-size:12px;color:#888}
.contents1_lst li:first-child a{background:none}
.contents1_txt img{width:100%;margin-top:21px}
.contents1_txt p{padding-top:11px}
.contents1_txt p a{margin:8px 10px 0 10px;font-size:14px;line-height:20px;color:#444}
.contents1_txt ul{clear:both;padding:10px 0;}
.contents1_txt ul a{text-overflow: ellipsis;white-space: nowrap;overflow: hidden;display:block;margin:0 10px;border-bottom:1px solid #ced2d7;font-size:12px;line-height:33px;color:#888}
.contents1_txt li:last-child a{border-bottom:none}
.contents1_area2{float:left;width:25%;margin-left:-1px;border:1px solid #ddd}
.contents1_area3{float:left;width:25%;margin-left:-1px;border:1px solid #ddd}
.contents1_area3 img{width:100%}
.consulting_lst li{float:left;position:relative;width:50%;text-align:center}
.consulting_lst li.on a{padding-bottom:10px;background:#fff;color: #666;font-weight: bold;}
.consulting_lst li.on span{position:absolute;left:50%;bottom:-5px;width:11px;height:6px;margin-left:-5.5px;background:url(../images/planer_tab_arrow.gif) 0 0 no-repeat}
.consulting_lst a{display:block;height:45px;border-top:1px solid #dedddc;border-bottom:1px solid #dedddc;background:#f6f6f6;font-size:14px;line-height:45px;color:#888}
.consulting_lst li:first-child a{border-right:1px solid #dedddc}
.contents1_area4{float:left;width:25%;margin-left:-1px;border:1px solid #ddd}
.contents1_area4 img{width:100%}
@media all and (max-width: 1220px){
.contents1_wrap{margin:49px 7px 0 10px}
}

