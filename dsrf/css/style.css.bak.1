@charset "euc-kr";
@import url("reset.css");
@import 'https://fonts.googleapis.com/css?family=Roboto:700';
*{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}
body{font-family:'Nanum Gothic',sans-serif}
.blind{overflow:hidden;position:absolute;top:0;left:0;width:1px;height:1px;text-indent:-9999px}

/* gnb */
.dsrf_logo{width:129px;margin:42px auto 0}
.dsrf_m_logo{display:none}
.menu_open{display:none}
.menu_close{display:none}
.gnb{overflow:hidden;width:640px;margin:0 auto}
.gnb li{float:left;width:160px;height:60px}
.gnb li a{display:block;font-size:16px;font-weight:700;line-height:60px;color:#393939;text-align:center}
.gnb li a:hover,.gnb li a:focus{color:#9c0b00}

@media all and (max-width: 767px){
.dsrf_logo{display:none}
.dsrf_m_logo{display:block;height:70px;background:#9c0b00;text-align:center}
.dsrf_m_logo a{font-size:18px;font-weight:700;line-height:70px;color:#fff}
.menu_open{display:block;position:absolute;top:13px;right:7px;width:45px;height:45px;padding:9px 0 0 7px}
.menu_open.on{display:none}
.menu_close{position:absolute;top:13px;right:7px;width:45px;height:45px;padding:9px 0 0 7px}
.menu_close.on{display:block}
.gnb{display:none;width:100%;background:#000}
.gnb li{clear:both;width:100%}
.gnb li:first-child{margin-top:20px}
.gnb li:last-child{margin-bottom:20px}
.gnb li a{color:#fff}
}

@media all and (min-width: 768px){
.gnb{display:block!important}
}

/* dsrf 메인 */
.dsrf{width:100%;height:695px;border-top:2px solid #9c0b00;background:url(../images/main_vis01.jpg) center center no-repeat;background-size:cover;-webkit-background-size:cover;-ms-behavior:url(../js/backgroundsize.min.htc);behavior:url(../js/backgroundsize.min.htc)}
.dsrf_tit{width:467px;height:221px;margin:141px auto 0;background:url(../images/dsrf_img.png) 0 0 no-repeat;text-indent:-9999px}
.dsrf_txt{margin-top:11px;padding:0 15px;font-size:14px;line-height:20px;color:#fff;text-align:center}
.intro_btn{display:block;width:198px;height:43px;margin:58px auto 0;border:2px solid #fff;font-weight:700;font-size:16px;line-height:43px;color:#fff;text-align:center}
.intro_btn:hover,.intro_btn:focus{background:#fff;color:#000}

@media all and (max-width: 1023px){
.dsrf_tit{width:365px;height:172px;margin:190px auto 0;background:url(../images/dsrf_img02.png) 0 0 no-repeat}
}
@media all and (max-width: 800px){
.dsrf{height:542px}
.dsrf_tit{margin:110px auto 0}
}
@media all and (max-width: 680px){
.dsrf_txt .br{display:none}
}
@media all and (max-width: 414px){
.dsrf{height:331px}
.dsrf_tit{display:none}
.dsrf_txt{margin-top:61px}
}

/* business */
.business_wrap{overflow:hidden}
.business_wrap h2{margin-top:46px;font-size:48px;font-family:'Roboto',sans-serif;font-weight:700;color:#393939;text-align:center}
.business_wrap .tit_icon{display:block;width:25px;height:22px;margin:21px auto 0;background:url(../images/icon01.png) 0 0 no-repeat}
.business{width:960px;margin:28px auto 0}
.business_area a{display:block;width:212px;height:327px;margin:0 14px;text-indent:-9999px}
.business01 a{background:url(../images/business_b01.png) 0 0 no-repeat}
.business02 a{background:url(../images/business_b02.png) 0 0 no-repeat}
.business03 a{background:url(../images/business_b03.png) 0 0 no-repeat}
.business04 a{background:url(../images/business_b04.png) 0 0 no-repeat}
.business05 a{background:url(../images/business_b05.png) 0 0 no-repeat}
.business06 a{background:url(../images/business_b06.png) 0 0 no-repeat}
.business_area a:hover,.business_area a:focus{background-position:0 -327px}
.business .slick-prev{left:-102px;z-index:10;width:68px;height:68px;background:url(../images/controls_prev.png) 0 0 no-repeat}
.business .slick-next{right:-102px;z-index:10;width:68px;height:68px;background:url(../images/controls_next.png) 0 0 no-repeat}

@media all and (max-width: 1200px){
.business{width:720px}
}
@media all and (max-width: 970px){
.business{width:480px}
}
@media all and (max-width: 740px){
.business{width:240px}
}
@media all and (max-width: 479px){
.business .slick-prev{left:0}
.business .slick-next{right:0}
}

/* about */
.about{overflow:hidden;margin-top:90px;padding:0 15px 90px;background:#ebebeb}
.about h2{margin-top:46px;font-size:48px;font-family:'Roboto',sans-serif;font-weight:700;color:#393939;text-align:center}
.about .tit_icon{display:block;width:25px;height:22px;margin:21px auto 0;background:url(../images/icon01.png) 0 0 no-repeat}
.about_area01{overflow:hidden;max-width:1164px;margin:26px auto 0}
.about_area02{overflow:hidden;max-width:1164px;margin:4px auto 0}
.about_area01 li{float:left}
.about_area01 li:first-child,.about_area01 li:nth-child(3){width:25%}
.about_area01 li:nth-child(2){width:50%}
.about_area02 li{float:left;width:25%}
.about_area01 img,.about_area02 img{width:100%}

@media all and (max-width: 620px){
.about_area01 li:first-child,.about_area01 li:nth-child(3),.about_area02 li{width:50%}
.about_area01 li:nth-child(2){display:none}
.about_area02 li:nth-child(even){display:none}
}

/* footer */
footer{overflow:hidden;padding-bottom:34px;background:#222328}
.footer{max-width:1164px;margin:52px auto 0}
.foot_logo{float:left}
address{overflow:hidden;margin-left:292px}
.address li{float:left;margin-right:30px;font-size:12px;line-height:18px;color:#a2a5b0}
.copyright{float:left;margin:39px 0 0 180px;font-size:12px;color:#a2a5b0}

@media all and (max-width: 1163px){
.foot_logo{display:none}
address{margin:0 15px}
.copyright{margin:39px 15px 0}
}

/* 채용정보 페이지 */

/* recruit_tit 채용정보 타이틀 */
.recruit_tit{overflow:hidden;width:100%;height:264px;background:url(../images/recruit_vis.jpg) center center no-repeat;background-size:cover;background-size:cover;-webkit-background-size:cover;-ms-behavior:url(../js/backgroundsize.min.htc);behavior:url(../js/backgroundsize.min.htc)}
.recruit_tit h2{margin-top:78px;padding-bottom:15px;background:url(../images/sub_tit_icon01.gif) center 49px no-repeat;font-size:36px;color:#fff;letter-spacing:-2px;text-align:center}
.recruit_txt{margin-top:26px;font-size:16px;line-height:16px;color:#fff;letter-spacing:-.5px;text-align:center}

/* 서브 네비게이션 */
.snb_wrap{width:100%;height:57px;border-bottom:1px solid #e4e4e4}
.snb{max-width:1164px;margin:0 auto}
.snb > li{float:left}
.snb > li:first-child{width:58px;height:56px;border-left:1px solid #e4e4e4;border-right:1px solid #e4e4e4;text-indent:-9999px}
.snb > li:first-child a{display:block;width:58px;height:56px;background:url(../images/home_icon.jpg) 0 -56px no-repeat}
.snb > li:first-child a:hover,.snb > li:first-child a:focus{background-position:0 0}
.snb > li:nth-child(2){width:202px;height:56px;border-right:1px solid #e4e4e4;background:url(../images/arrow_icon01.gif) 166px center no-repeat}
.snb > li.on:nth-child(2){background:url(../images/arrow_icon02.gif) 166px center no-repeat}
.snb > li:nth-child(2) > a{display:block;padding-left:24px;font-size:15px;line-height:57px;font-weight:bold;color:#727272;text-decoration:none}
.snb > li > ul{display:none;height:166px;padding-top:15px;border:1px solid #e4e4e4;border-top:none;background:#fff}
.snb > li > ul a{display:block;height:35px;padding-left:24px;line-height:35px;font-size:15px;font-weight:bold;color:#727272;text-decoration:none}
.snb > li a:hover,.snb > li a:focus{color:#9c0b00}
.snb > li.on > ul{display:block;position:relative;z-index:10}

@media all and (max-width: 767px){
.snb_wrap{display:none}
}

/* 모집부문 업무 관련사항 */
.recruit_con01_wrap{overflow:hidden;clear:both;max-width:1194px;margin:0 auto;padding:0 15px}
.recruit_con01_wrap h3{margin-top:54px;font-size:24px;color:#252020;letter-spacing:-1px}
.recruit_con01_wrap h3 span{color:#9c0600}
.recruit_con01{width:100%;max-width:1164px;margin-top:20px}
.recruit_con01 .col1{width:13.31615120275%}
.recruit_con01 .col2{width:29.29553264605%}
.recruit_con01 .col3{width:35.30927835052%}
.recruit_con01 .col4{width:11.0824742268%}
.recruit_con01 thead{background:#f7f8fa}
.recruit_con01 th{height:66px;border:1px solid #e4e4e4;font-size:16px;font-weight:bold;color:#4c4a4d;vertical-align:middle}
.recruit_con01 td{height:96px;padding-left:20px;border:1px solid #e4e4e4;border-top:none;font-size:16px;line-height:20px;color:#4c4a4d;vertical-align:middle}
.recruit_con01 td:first-child,.recruit_con01 td:nth-child(4),.recruit_con01 td:nth-child(5){padding-left:0;text-align:center}
.recruit_m_con01{display:none}

@media all and (max-width: 800px){
.recruit_con01{display:none}
.recruit_m_con01{display:block;overflow:hidden;width:100%;max-width:1164px;margin-top:10px;padding:10px 0}
.recruit_m_con01 tbody{display:table;width:100%;border:1px solid #e4e4e4;background:#f7f8fa}
.recruit_m_con01 th{width:40%;padding-left:10px;font-size:16px;line-height:30px;font-weight:bold;color:#4c4a4d;text-align:left}
.recruit_m_con01 tr:first-child th{padding-top:10px}
.recruit_m_con01 tr:last-child th{padding-bottom:10px}
.recruit_m_con01 td{width:60%;font-size:16px;line-height:20px;color:#4c4a4d}
}

/* 근무조건 */
.recruit_con02_wrap{overflow:hidden;max-width:1194px;margin:0 auto;padding:0 15px;}
.recruit_con02_wrap h3{margin-top:54px;font-size:24px;color:#9c0600;letter-spacing:-1px}
.recruit_con02{max-width:1164px;width:100%;margin-top:20px;}
.recruit_con02 .col1{width:11.94158075601%}
.recruit_con02 .col2{width:57.21649484536%}
.recruit_con02 .col3{width:8.5910652921%}
.recruit_con02 .col4{width:22.25085910653%}
.recruit_con02 tbody{border:1px solid #e4e4e4;border-left: none;}
.recruit_con02 th{border-left: 1px solid #e4e4e4;border-right: 1px solid #e4e4e4;height:168px;background:#f7f8fa;font-size:16px;font-weight:bold;line-height:24px;color:#4c4a4d;vertical-align:middle}
.recruit_con02 td{vertical-align: middle;font-size: 16px;line-height: 24px;color: #4c4a4d;padding: 0 20px;}

@media all and (max-width: 800px){
.recruit_con02 tbody{border: 1px solid #e4e4e4;border-top: none;}
.recruit_con02 th{display: block;height: auto;border: 1px solid #e4e4e4;border-left: none;border-right:none;padding: 20px 0;}
.recruit_con02 td{display: block;height: auto;padding: 20px 20px;}
}

/* 복리후생 제도 */
.recruit_con03_wrap{overflow: hidden;max-width: 1194px;margin: 0 auto;padding: 0 15px;}
.recruit_con03_wrap h3{margin-top:54px;font-size:24px;color: #252020;letter-spacing:-1px}
.recruit_con03_wrap h3 span{color:#9c0600;}
.recruit_con03{margin-top: 20px;padding: 20px 15px;background: #f7f8fa;overflow: hidden;}
.recruit_con03_area li{float: left;width: 20%;text-align: center;font-size: 14px;font-weight: bold;color: #4c4a4d;padding-top: 103px;}
.recruit_con03_area li:first-child{background: url(../images/recruit_icon01.png) center 0 no-repeat;}
.recruit_con03_area li:nth-child(2){background: url(../images/recruit_icon02.png) center 0 no-repeat;}
.recruit_con03_area li:nth-child(3){background: url(../images/recruit_icon04.png) center 0 no-repeat;}
.recruit_con03_area li:nth-child(4){background: url(../images/recruit_icon04.png) center 0 no-repeat;}
.recruit_con03_area li:nth-child(5){background: url(../images/recruit_icon05.png) center 0 no-repeat;}

@media all and (max-width: 800px){
.recruit_con03_area li{width: 33.3%;}
.recruit_con03_area li:nth-child(4){margin-top: 40px;}
.recruit_con03_area li:nth-child(5){margin-top: 40px;}
}

@media all and (max-width: 459px){
.recruit_con03_area li{width: 50%;}
.recruit_con03_area li:nth-child(3){margin-top: 40px;}
}

/* 근무지역 */
.recruit_con04_wrap{overflow: hidden;max-width: 1194px;margin: 0 auto;padding:0 15px;}
.recruit_con04_wrap h3{margin-top:54px;font-size:24px;color: #9c0600;letter-spacing:-1px}
#map{width: 100%;height: 400px;margin-top: 23px;}
.address02{font-size: 16px;color:#4c4a4d;margin-top: 19px;}
.address02 span{margin-right: 20px;font-weight: bold;}
.recruit_btn{width: 290px;height: 60px;margin: 55px auto 70px;background: #9c0b00;line-height: 60px;color: #fff;font-weight: bold;font-size: 16px;display: block;text-align: center;}
.recruit_btn:hover, .recruit_btn:focus{background: #00439c;}